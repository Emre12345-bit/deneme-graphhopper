    { "if": "road_environment == BRIDGE", "multiply_by": "1.15" },
    { "if": "road_class == FOOTWAY", "multiply_by": "1.1" },
    { "if": "road_class == PEDESTRIAN", "multiply_by": "1.12" },
    { "if": "crossing == MARKED", "multiply_by": "1.2" },
    { "if": "crossing == TRAFFIC_SIGNALS", "multiply_by": "1.25" },
    { "if": "crossing == UNCONTROLLED", "multiply_by": "1.18" }
  ],
  "speed": [
    { "if": "true", "limit_to": "foot_average_speed" },
    { "if": "road_environment == BRIDGE", "limit_to": "6" }
  ]
